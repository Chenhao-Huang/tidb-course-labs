{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "TiDBDemoLambdaPermission01",
      "Effect": "Allow",
      "Action": "sts:GetCallerIdentity",
      "Resource": "*"
    },
    {
      "Sid": "TiDBDemoLambdaPermission02",
      "Effect": "Allow",
      "Action": [
        "elasticloadbalancing:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "TiDBDemoLambdaPermission03",
      "Effect": "Allow",
      "Action": [
        "ec2:Describe*",
        "ec2:List*",
        "ec2:DeleteTags",
        "ec2:CreateTags"
      ],
      "Resource": "*"
    },
    {
      "Sid": "TiDBDemoLambdaPermission04",
      "Effect": "Allow",
      "Action": [
        "sqs:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "TiDBDemoLambdaPermission05",
      "Effect": "Allow",
      "Action": [
        "logs:*"
      ],
      "Resource": "*"
    }
  ]
}